@{
    Name = "DarkRounded"
    Definition = @{
        UDDashboard = @{
            BackgroundColor = "#333333"
            FontColor = "#FFFFF"
              }
              UDNavBar = @{
            BackgroundColor = "#1c1c1c"
            FontColor = "#55b3ff"
              }
              UDCard= @{
